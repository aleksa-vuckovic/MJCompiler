program MJProgram

	namespace colors {
		const int red = 0;
		const int green = 1;
		const int yellow = 2;
		{}
	}

	class String {
		static int max;
		static {
			max = 10;
		}
		
		char val[];
		int len;
		
		{
			String() {
				val = new char[max];
				len = 0;
			}
			
			void addChar(char c) {
				if (len >= max) return;
				val[len] = c;
				len++;
			}
			void add2Chars(char c1, char c2) {
				addChar(c1); addChar(c2);
			}
			void add3Chars(char c1, char c2, char c3) {
				add2Chars(c1, c2); addChar(c3);
			}
			void prnt() int i; {
				for(i = 0; i < len; i++) {
					print(val[i]);
				}
			}
		}
	}
	
	class Vehicle {
	
		static String red, green, yellow;
		static {
			red = new String();
			red.add3Chars('R', 'e', 'd');
			
			green = new String();
			green.add3Chars('G', 'r', 'e'); green.add2Chars('e', 'n');
			
			yellow = new String();
			yellow.add3Chars('Y', 'e', 'l'); yellow.add3Chars('l', 'o', 'w');
		}
		
		int color;
		String name;
		
		{
			char getType() {
				return 'N';
			}
			
			void prntColor() {
				if (color == colors::red) red.prnt();
				else if (color == colors::green) green.prnt();
				else if (color == colors::yellow) yellow.prnt();
				else print('/');
			}
			void prnt() {
				print(getType());
				print('(');
				if (name != null) {
					name.prnt();
					print(',');
				}
				prntColor();
				print(')');
				print('\n');
			}
		}
		
	}
	
	class Car extends Vehicle {
		
		{
			Car(String name, int color) {
				this.name = name;
				this.color = color;
			}
			
			char getType() {
				return 'C';
			}
		}
	}
	
	class Bus extends Vehicle {
		{
			Bus(String name, int color) {
				this.name = name;
				this.color = color;
			}
			
			//char getType(char a) { return 'B'; }
			char getType() {
				return 'B';
			}
		}
	}
{
	void main() String a, b; Vehicle v1, v2; {
		a = new String();
		b = new String();
		
		a.add3Chars('K', 'I', 'A');
		b.add3Chars('M', 'e', 'r'); b.add3Chars('c', 'e', 'd'); b.add2Chars('e', 's');
		
		v1 = new Car(a, colors::red);
		v2 = new Bus(b, colors::yellow);
		
		v1.prnt();
		v2.prnt();
		
		return;
	}
}