program MJProgram
	
	class String {
		static int max;
		static {
			max = 10;
		}
		
		char val[];
		int len;
		
		{
			String() {
				val = new char[max];
				len = 0;
			}
			//String() {}
			
			void addChar(char c) {
				if (len >= max) return;
				val[len] = c;
				len++;
			}
			void add2Chars(char c1, char c2) {
				addChar(c1); addChar(c2);
			}
			void add3Chars(char c1, char c2, char c3) {
				add2Chars(c1, c2); addChar(c3);
			}
			void prnt() int i; {
				for(i = 0; i < len; i++) {
					print(val[i]);
				}
				print('\n');
			}
		}
	}
	
{
	void main() String a, b, c;{
		a = new String();
		b = new String();
		c = new String();

		a.add3Chars('H', 'e', 'l');
		a.add3Chars('l', 'o', ' ');
		b.add3Chars('W', 'o', 'r');
		b.add3Chars('l', 'd', '!');
		c.add2Chars(':', ')');
		a.prnt();
		b.prnt();
		c.prnt();
		
		return;
		//return 10;
	}
}