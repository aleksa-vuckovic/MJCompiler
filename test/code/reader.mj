program MJProgram
	class String {
		static int max;
		static {
			max = 10;
		}
		
		char val[];
		int len;
		
		{
			String() {
				val = new char[max];
				len = 0;
			}
			
			void addChar(char c) {
				if (len >= max) return;
				val[len] = c;
				len++;
			}
			void add2Chars(char c1, char c2) {
				addChar(c1); addChar(c2);
			}
			void add3Chars(char c1, char c2, char c3) {
				add2Chars(c1, c2); addChar(c3);
			}
			void prnt() int i; {
				for(i = 0; i < len; i++) {
					print(val[i]);
				}
			}
			void clear() {
				len = 0;
			}
		}
	}
{
	void main() String prompt, input; char x; {
		prompt = new String();
		prompt.add3Chars('>','>','>');
		
		input = new String();
		
		prompt.prnt();
		for(;;) {
			read(x);
			input.addChar(x);
			if (x == '\n') {
				input.prnt();
				input.clear();
				prompt.prnt();
			}
		}
		
		
	}
}